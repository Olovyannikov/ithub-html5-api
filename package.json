{
  "name": "ithub-html5-api",
  "version": "0.0.0",
  "author": "Ilya Olovyannikov",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "commit": "npm run lint:ts && npm run lint:css && cz",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:css": "npx stylelint \"src/**/*.css\"",
    "lint:css:fix": "npx stylelint \"**/*.css\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:staged": "vitest run"
  },
  "dependencies": {
    "@farfetched/core": "^0.12.4",
    "@mantine/core": "^7.13.0",
    "@mantine/hooks": "^7.13.0",
    "@mantine/notifications": "^7.13.0",
    "@pbe/react-yandex-maps": "^1.2.5",
    "@tabler/icons-react": "^3.19.0",
    "@types/lodash-es": "^4.17.12",
    "@withease/web-api": "^1.3.0",
    "effector": "^23.2.2",
    "effector-react": "^23.2.1",
    "effector-storage": "^7.1.0",
    "normalize.css": "^8.0.1",
    "ofetch": "^1.4.0",
    "patronum": "^2.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.26.2",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.7.26",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "clsx": "^2.1.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "effector-swc-plugin": "^0.5.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-ts-react-important-stuff": "3.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-hooks": "0.4.3",
    "eslint-plugin-i18next": "6.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.47",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.6.2",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.4.8",
    "vite-css-modules": "^1.4.2"
  },
  "lint-staged": {
    "*.css": "npm run lint:css:fix",
    "*.{js,jsx,ts,tsx}": "npm run lint:ts",
    "*.{js,jsx,ts,tsx,json,css,md}": "npx prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
